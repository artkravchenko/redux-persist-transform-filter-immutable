{
    "name": "@artkravchenko/redux-persist-transform-filter-immutable",
    "description": "Filter transformator for redux-persist supporting immutable.js",
    "keywords": [
        "react",
        "redux",
        "redux-persist",
        "redux-persist-transform",
        "transform",
        "filter",
        "immutable"
    ],
    "license": "MIT",
    "version": "1.0.1",
    "author": {
        "name": "Gabriel Schuster - actra.development",
        "email": "opensource@actra.de",
        "url": "http://www.actra.de/velopment"
    },
    "homepage": "https://github.com/artkravchenko/redux-persist-transform-filter-immutable#readme",
    "repository": {
        "type": "git",
        "url": "git@github.com:artkravchenko/redux-persist-transform-filter-immutable.git"
    },
    "bugs": "https://github.com/artkravchenko/redux-persist-transform-filter-immutable/issues",
    "publishConfig": {
        "registry": "https://registry.npmjs.com/",
        "access": "public"
    },
    "dependencies": {
        "immutable": "^4.0.0-rc.9",
        "lodash.forin": "^4.4.0",
        "lodash.get": "^4.4.2",
        "lodash.includes": "^4.3.0",
        "lodash.isempty": "^4.4.0",
        "lodash.isobject": "^3.0.2",
        "lodash.isstring": "^4.0.1",
        "lodash.isundefined": "^3.0.1",
        "lodash.pickby": "^4.6.0",
        "lodash.set": "^4.3.2",
        "lodash.unset": "^4.5.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "mocha": "^5.1.1",
        "mkdirp": "^0.5.1",
        "redux": "^4.0.0",
        "rimraf": "^2.6.2",
        "rollup": "^1.32.0",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-node-resolve": "^5.2.0"
    },
    "peerDependencies": {
        "immutable": "^4.0.0-rc.9"
    },
    "files": [
        "dist",
        "es"
    ],
    "main": "dist/index.js",
    "module": "es/index.js",
    "sideEffects": false,
    "scripts": {
        "test": "BABEL_ENV=test npm run-script mocha -- --require babel-register spec.js",
        "build": "BABEL_ENV=release npm run-script rimraf -- ./dist ./es && npm run-script mkdirp -- dist && rollup -c",
        "release": "npm run-script test && npm run-script build && npm publish --access public",
        "mkdirp": "./node_modules/.bin/mkdirp",
        "mocha": "./node_modules/.bin/mocha",
        "rimraf": "./node_modules/.bin/rimraf"
    }
